{
  "For Loop": {
    "prefix": "for ...",
    "body": [
      "<% for (var ${1:i} = 0, len = ${2:array}.length; ${1:i} < len; ${1:i}++) { %>",
      "\t$0",
      "<% } %>"
    ]
  },
  "Output Escaped": {
    "prefix": "output escaped ...",
    "body": [
      "<%= ${1:variable} %>"
    ]
  },
  "Output Unescaped": {
    "prefix": "output unescaped ...",
    "body": [
      "<%- ${1:variable} %>"
    ]
  },
  "Comment": {
    "prefix": "comment ...",
    "body": [
      "<%# ${1:comment} %>"
    ]
  }
}
